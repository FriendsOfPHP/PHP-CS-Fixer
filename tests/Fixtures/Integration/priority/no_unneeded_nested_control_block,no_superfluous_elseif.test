--TEST--
Integration of fixers: no_unneeded_nested_control_block,no_superfluous_elseif.
--RULESET--
{"no_unneeded_nested_control_block": true, "no_superfluous_elseif": true}
--EXPECT--
<?php
if (1) {
    return 1;
}
if (2) {

        echo 2;

}

--INPUT--
<?php
if (1) {
    return 1;
} else {
    if (2) {
        echo 2;
    }
}
